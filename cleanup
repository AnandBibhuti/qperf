rm -r -f .deps autom4te.cache
rm -f `cat <<EOF
    AUTHORS
    ChangeLog
    INSTALL
    Makefile
    Makefile.in
    NEWS
    README
    aclocal.m4
    config.log
    config.status
    configure
    depcomp
    install-sh
    missing
    src/Makefile
    src/Makefile.in
EOF`
